@model CBMS.Models.Roster.StaffProfilePaymentDetailModel

<fieldset>
    <legend>Payment Details</legend>
    <div class="form-group">
        @Html.LabelFor(model => model.AcName, htmlAttributes: new { @class = "control-label col-md-3" })
        <div class="col-md-3">
            @Html.EditorFor(model => model.AcName, new { htmlAttributes = new { @class = "form-control" } })
            @Html.ValidationMessageFor(model => model.AcName, "", new { @class = "text-danger" })
        </div>
        @Html.LabelFor(model => model.BSB, htmlAttributes: new { @class = "control-label col-md-3" })
        <div class="col-md-3">
            @Html.EditorFor(model => model.BSB, new { htmlAttributes = new { @class = "form-control" } })
            @Html.ValidationMessageFor(model => model.BSB, "", new { @class = "text-danger" })
        </div>
        @Html.LabelFor(model => model.AcNumber, htmlAttributes: new { @class = "control-label col-md-3" })
        <div class="col-md-3">
            @Html.EditorFor(model => model.AcNumber, new { htmlAttributes = new { @class = "form-control" } })
            @Html.ValidationMessageFor(model => model.AcNumber, "", new { @class = "text-danger" })
        </div>
        @Html.LabelFor(model => model.ConfigBankModelId, "Bank", htmlAttributes: new { @class = "control-label col-md-3" })
        <div class="col-md-3">
            @Html.DropDownList("ConfigBankModelId", (SelectList)ViewBag.ConfigBankModelId, new { @class = "form-control" })
            @Html.ValidationMessageFor(model => model.ConfigBankModelId, "", new { @class = "text-danger" })
        </div>
    </div>
</fieldset>
